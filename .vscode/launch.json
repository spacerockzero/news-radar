	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Mocha tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/functions/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": ["-t", "10000", "test/*-test.js"],
			"cwd": "${workspaceRoot}/functions/",
			"preLaunchTask": null,
			"runtimeExecutable": "/home/jakob/.nvm/versions/node/v6.12.0/bin/node",
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		}
	]
}
